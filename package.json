  "scripts": {
    "prepublishOnly": "npm run build",
    "postpublish": "git push origin master",
    "test": "jest",
    "storybook:build": "build-storybook -o ./storybook/storybook-static -s ./storybook/assets -c ./storybook/.storybook",
    "storybook:dev": "cd storybook; start-storybook -s ./assets -p 6006",
    "snapshot": "percy storybook ./storybook/storybook-static",
    "clean": "rimraf _dist/",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -p tsconfig.json && tsc -p tsconfig.browser.json",
    "type-check": "tsc --noEmit",
    "dev": "tsc -p tsconfig.browser.json --watch",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check .",
    "eslint:run": "node_modules/.bin/eslint ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "npm run prettier:fix",
      "git add"
    ]
  },
